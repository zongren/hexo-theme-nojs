<div class="pagination">
  <span class="pagination__wrapper">
  <% if (page.prev){ %>
    <a class="pagination__link" href="<%- url_for(page.prev_link) %>" title="prev">prev</a>
  <% } %>
  <% if (page.next){ %>
    <a class="pagination__link" href="<%- url_for(page.next_link) %>" title="next">next</a>
  <% } %>
  </span>
</div>