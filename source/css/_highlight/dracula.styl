pre {
  display: block;
  overflow-x: auto;
  padding 10px content-padding;
  background: #282a36;
}

pre .keyword,
pre .selector-tag,
pre .literal,
pre .section,
pre .link {
  color: #8be9fd;
}

pre .function pre .keyword {
  color: #ff79c6;
}

pre,
pre .subst {
  color: #f8f8f2;
}

pre .string,
pre .title,
pre .name,
pre .type,
pre .attribute,
pre .symbol,
pre .bullet,
pre .addition,
pre .variable,
pre .template-tag,
pre .template-variable {
  color: #f1fa8c;
}

pre .comment,
pre .quote,
pre .deletion,
pre .meta {
  color: #6272a4;
}

pre .keyword,
pre .selector-tag,
pre .literal,
pre .title,
pre .section,
pre .doctag,
pre .type,
pre .name,
pre .strong {
  font-weight: bold;
}

pre .emphasis {
  font-style: italic;
}
