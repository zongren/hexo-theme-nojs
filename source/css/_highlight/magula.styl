pre {
  display: block;
  overflow-x: auto;
  padding 10px content-padding;
  background-color: #f4f4f4;
}

pre,
pre .subst {
  color: black;
}

pre .string,
pre .title,
pre .symbol,
pre .bullet,
pre .attribute,
pre .addition,
pre .variable,
pre .template-tag,
pre .template-variable {
  color: #050;
}

pre .comment,
pre .quote {
  color: #777;
}

pre .number,
pre .regexp,
pre .literal,
pre .type,
pre .link {
  color: #800;
}

pre .deletion,
pre .meta {
  color: #00e;
}

pre .keyword,
pre .selector-tag,
pre .doctag,
pre .title,
pre .section,
pre .built_in,
pre .tag,
pre .name {
  font-weight: bold;
  color: navy;
}

pre .emphasis {
  font-style: italic;
}

pre .strong {
  font-weight: bold;
}
