pre {
  display: block;
  overflow-x: auto;
  padding 10px content-padding;
  background: white;
  color: black;
}

pre .comment,
pre .quote {
  color: #800;
}

pre .keyword,
pre .selector-tag,
pre .section,
pre .title,
pre .name {
  color: #008;
}

pre .variable,
pre .template-variable {
  color: #660;
}

pre .string,
pre .selector-attr,
pre .selector-pseudo,
pre .regexp {
  color: #080;
}

pre .literal,
pre .symbol,
pre .bullet,
pre .meta,
pre .number,
pre .link {
  color: #066;
}

pre .title,
pre .doctag,
pre .type,
pre .attr,
pre .built_in,
pre .builtin-name,
pre .params {
  color: #606;
}

pre .attribute,
pre .subst {
  color: #000;
}

pre .formula {
  background-color: #eee;
  font-style: italic;
}

pre .selector-id,
pre .selector-class {
  color: #9B703F
}

pre .addition {
  background-color: #baeeba;
}

pre .deletion {
  background-color: #ffc8bd;
}

pre .doctag,
pre .strong {
  font-weight: bold;
}

pre .emphasis {
  font-style: italic;
}
