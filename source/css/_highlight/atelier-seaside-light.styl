/* Base16 Atelier Seaside Light - Theme */
/* by Bram de Haan (http://atelierbram.github.io/syntaxpreing/atelier-schemes/seaside) */
/* Original Base16 color scheme by Chris Kempson (https://github.com/chriskempson/base16) */

/* Atelier-Seaside Comment */
pre .comment,
pre .quote {
  color: #687d68;
}

/* Atelier-Seaside Red */
pre .variable,
pre .template-variable,
pre .attribute,
pre .tag,
pre .name,
pre .regexp,
pre .link,
pre .name,
pre .selector-id,
pre .selector-class {
  color: #e6193c;
}

/* Atelier-Seaside Orange */
pre .number,
pre .meta,
pre .built_in,
pre .builtin-name,
pre .literal,
pre .type,
pre .params {
  color: #87711d;
}

/* Atelier-Seaside Green */
pre .string,
pre .symbol,
pre .bullet {
  color: #29a329;
}

/* Atelier-Seaside Blue */
pre .title,
pre .section {
  color: #3d62f5;
}

/* Atelier-Seaside Purple */
pre .keyword,
pre .selector-tag {
  color: #ad2bee;
}

pre {
  display: block;
  overflow-x: auto;
  background: #f4fbf4;
  color: #5e6e5e;
  padding 10px content-padding;
}

pre .emphasis {
  font-style: italic;
}

pre .strong {
  font-weight: bold;
}
